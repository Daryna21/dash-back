datasource db {
  provider = "sqlite"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
// Internal tables
model Role {
  id  Int @id
  name  String
  created DateTime? @default(now()) 
}
// Have no direct dependendencies 'graphql/Schema.ts'{SameName}
model User {
  id  Int @id @default(autoincrement())
  email String
  password String
  name String?
  role Int
  Role Role @relation(fields: [role], references: [id])
  lastLogin  DateTime?
  created  DateTime? @default(now())
  updated  DateTime? @default(now())
}
model Brand {
  id  Int @id @default(autoincrement())
  url String
  email String
  created  DateTime? @default(now())
  updated  DateTime? @default(now())
  avgTimeStory  Float? @default(0)
  avgAllTimeStory Float? @default(0)
}
model Author {
  id  Int @id @default(autoincrement())
  url String
  email String
  isPublished Boolean? @default(false)
  created  DateTime? @default(now())
  edited  DateTime? @default(now())
  updated  DateTime? @default(now())
  published  DateTime? @default(now())
  avgTimeStory  Float? @default(0)
  avgAllTimeStory Float? @default(0)
}
model Editor {
  id  Int @id @default(autoincrement())
  name String
  edited  Int? @default(0)
  editedPercent Float? @default(0)
  published Int? @default(0)
  publishedPercent Float? @default(0)
  rejected Int? @default(0)
  rejectedPercent Float? @default(0)
  created  DateTime? @default(now())
  updated  DateTime? @default(now())
}